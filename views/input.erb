<form action="/" method="post">
  <div class="form_group">
    <label for="input_term">Term: </label>
    <input class="form-control" type="text" name="input_term" id="input_term">
  </div>
  <div class="form_group">
    <label for="input_definition">Definition: </label>
    <input class="form-control" type="text" name="input_definition" id="input_definition">
  </div>
  <button type="submit" class="btn btn-info">Submit!</button>
</form>

<h1>Terms</h1>
<% @list_of_words.each do |term|%>
<ul>
  <a href="/output/<%= @term.id %>"><%= term.term %></a>
</ul>


<% end %>
